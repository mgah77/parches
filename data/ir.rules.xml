<odoo>
    <data noupdate="1">
        <!-- Regla para ocultar el menú -->
        <record id="hide_menu_for_group_config" model="ir.rule">
            <field name="name">Ocultar menú</field>
            <field name="model_id" ref="base.model_ir_ui_menu"/>
            <field name="global" eval="True"/>
            <field name="domain_force">[('id', '!=', ref('stock.menu_warehouse_report'))]</field>
            <field name="groups" eval="[(6, 0, [ref('parches.group_bodega')])]"/>
        </record>
    </data>
</odoo>
